{
  "name": "@like-cake/ast-types",
  "version": "1.0.0",
  "description": "AST type definitions and schemas for E2E test automation",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.mjs",
      "require": "./dist/schemas/index.js"
    },
    "./guards": {
      "types": "./dist/guards/index.d.ts",
      "import": "./dist/guards/index.mjs",
      "require": "./dist/guards/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check src",
    "lint:fix": "biome check --write --unsafe src",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@like-cake/tsconfig": "*",
    "tsup": "~8.5.0",
    "typescript": "~5.9.2",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "zod": ">=3.20.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  }
}
